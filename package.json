{
  "name": "nativescript-vscode-extension",
  "displayName": "NativeScript",
  "version": "0.0.1",
  "icon": "images/icon.png",
  "description": "A Visual Studio Code extension for NativeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/NativeScript/vscode-chrome-debug"
  },
  "publisher": "Telerik",
  "bugs": "https://github.com/NativeScript/vscode-chrome-debug/issues",
  "engines": {
    "vscode": "0.10.x"
  },
  "categories": ["Debuggers"],
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "source-map": "^0.5.3",
    "nativescript": "^1.5.1",
    "shelljs": "0.5.3"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-mocha": "^2.1.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-tslint": "^3.3.1",
    "gulp-typescript": "^2.8.0",
    "gulp-util": "^3.0.5",
    "mocha": "^2.3.3",
    "mockery": "^1.4.0",
    "sinon": "^1.17.2",
    "tsd": "^0.6.3",
    "tslint": "^2.5.1",
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "main": "./out/nativescript/nsMain",
  "activationEvents": [
    "onCommand:nativescript.create",
    "onCommand:nativescript.build",
    "onCommand:nativescript.run",
    "onCommand:nativescript.livesync"
  ],
  "contributes": {
    "commands": [
      {
        "command": "nativescript.create",
        "title": "Create",
        "category": "NativeScript"
      },
      {
          "command": "extension.createProject",
          "title": "NativeScript: Create a new project"
      },
      {
        "command": "nativescript.build",
        "title": "Build",
        "category": "NativeScript"
      },
      {
        "command": "nativescript.run",
        "title": "Run",
        "category": "NativeScript"
      },
      {
        "command": "nativescript.livesync",
        "title": "LiveSync",
        "category": "NativeScript"
      }
    ],
    "debuggers": [
      {
        "type": "nativescript",
        "label": "NativeScript",
        "enableBreakpointsFor": {
          "languageIds": [
            "javascript",
            "typescriptreact"
          ]
        },
        "program": "./out/webkit/webkitDebug.js",
        "runtime": "node",
        "initialConfigurations": [
          {
            "name": "Launch on iOS",
            "type": "nativescript",
            "platform": "ios",
            "request": "launch",
            "sourceMaps": true,
            "diagnosticLogging": false,
            "stopOnEntry": true,
            "emulator": true
          },
          {
            "name": "Attach on iOS",
            "type": "nativescript",
            "platform": "ios",
            "request": "attach",
            "sourceMaps": false,
            "diagnosticLogging": false,
            "emulator": true
          },
          {
            "name": "Launch on Android Device",
            "type": "nativescript",
            "platform": "android",
            "request": "launch",
            "sourceMaps": true,
            "diagnosticLogging": false,
            "stopOnEntry": true,
            "emulator": false
          },
          {
            "name": "Launch on Android Emulator",
            "type": "nativescript",
            "platform": "android",
            "request": "launch",
            "webRoot": ".",
            "sourceMaps": true,
            "diagnosticLogging": false,
            "stopOnEntry": true,
            "emulator": true
          },
          {
            "name": "Attach on Android Device",
            "type": "nativescript",
            "platform": "android",
            "request": "attach",
            "sourceMaps": false,
            "diagnosticLogging": false,
            "emulator": false
          },
          {
            "name": "Attach on Android Emulator",
            "type": "nativescript",
            "platform": "android",
            "request": "attach",
            "sourceMaps": false,
            "diagnosticLogging": false,
            "emulator": true
          }
        ],
        "configurationAttributes": {
          "launch": {
            "required": [],
            "properties": {
              "runtimeArgs": {
                "type": "array",
                "description": "Optional arguments passed to the runtime executable.",
                "items": {
                  "type": "string"
                },
                "default": []
              },
              "sourceMaps": {
                "type": "boolean",
                "description": "Use JavaScript source maps (if they exist).",
                "default": true
              },
              "diagnosticLogging": {
                "type": "boolean",
                "description": "When true, the adapter logs its own diagnostic info to the console",
                "default": false
              },
              "platform": {
                "type": "string",
                "description": "NativeScript platform",
                "default": null
              },
              "emulator": {
                  "type": "boolean",
                  "description": "Whether the app to run in emulator or on a physical device.",
                  "default": false
              }
            }
          },
          "attach": {
            "required": [],
            "properties": {
              "sourceMaps": {
                "type": "boolean",
                "description": "Use JavaScript source maps (if they exist).",
                "default": true
              },
              "diagnosticLogging": {
                "type": "boolean",
                "description": "When true, the adapter logs its own diagnostic info to the console",
                "default": false
              },
              "platform": {
                "type": "string",
                "description": "NativeScript platform",
                "default": null
              },
              "emulator": {
                  "type": "boolean",
                  "description": "Whether the app to run in emulator or on a physical device.",
                  "default": false
              }
            }
          }
        }
      }
    ]
  }
}
